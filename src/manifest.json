{
  "manifest_version": 3,
  "name": "Example manifest",
  "version": "0.0.1",
  "description": "A basic example extension",
  "permissions": [],
  "icons": {
    "48": "static/icon.svg",
    "128": "static/icon.svg"
  },
  "options_page": "options/options.html",
  "action": {
    "default_title": "Example extension popup",
    "default_popup": "popup/popup.html"
  },
  "background": {
    "scripts": [
      "background/background.js"
    ],
    "type": "module"
  },
  "content_scripts": [
    {
      "js": ["content_scripts/example.js"],
      "matches": ["http*://example.com"]
    }
  ]
}
